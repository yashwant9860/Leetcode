class Solution {
public:
    string getHint(string secret, string guess) {
        
        int l1=secret.length(),l2=guess.length(),x=0;
        vector<int>v1(10,0),v2(10,0);
        for(int i=0;i<l1;i++)
        {
            if(secret[i]==guess[i])
            {   
            x++;
            }
            else
            {
            v1[secret[i]-'0']++;
            v2[guess[i]-'0']++;
            }
        }
        int y=0;
        for(int i=0;i<10;i++)
        {
            y+=min(v1[i],v2[i]);
        }
        string ans=to_string(x);
        ans=ans+'A';
        string ans1=to_string(y);
        ans=ans+ans1;
        ans=ans+'B';
        
        return ans;
    }
};
