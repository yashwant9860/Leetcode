class Solution {
public:
    vector<string> findRepeatedDnaSequences(string s) {
        vector<string>v,ans;
        v.push_back("");
        int n=s.length();
        if(n<=10)
        {
            return ans;
        }
        map<string,int>m;
        for(int i=0;i<n-9;i++)
        {
            v.push_back(s.substr(i,10));
            if(m.find(v[i+1])!=m.end())
            {
                m[v[i+1]]++;
            }
            else
            {
                m.insert({v[i+1],1});
            }
        }
        for(int i=0;i<v.size();i++)
        {
            if(m[v[i]]>1)
            {
                ans.push_back(v[i]);
                m[v[i]]=0;
            }
        }
        return ans;
    }
};
