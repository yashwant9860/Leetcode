class Solution {
public:
    bool winnerOfGame(string colors) {
        int n=colors.size(),a=0,b=0;
        for(int i=1;i<n-1;i++)
        {
            if(colors[i-1]=='A'&&colors[i+1]=='A'&&colors[i]=='A')
                a++;
            if(colors[i-1]=='B'&&colors[i+1]=='B'&&colors[i]=='B')
                b++;
        }
        return a>b;
    }
};
