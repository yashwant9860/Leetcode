class Solution {
public:
    int maximalSquare(vector<vector<char>>& matrix) {
        int m=matrix.size(),n=matrix[0].size();
        int dp[m+1][n+1],best=0;;
        memset(dp,0,sizeof dp);
        for(int i=1;i<m+1;i++)
        {
            for(int j=1;j<n+1;j++)
            {
                if(matrix[i-1][j-1]=='1')
                dp[i][j]=min(min(dp[i][j-1],dp[i-1][j]),dp[i-1][j-1])+1;
                best=max(best,dp[i][j]);
            }
        }
        return best*best;
    }
};
