class Solution {
public:
    vector<int> decode(vector<int>& encoded) {
        int s=0,n=encoded.size()+1,l=0,f=0;
        vector<int>v;
        v.push_back(encoded[0]);
        s+=v[0];
        for(int i=1;i<=n;i++)
        {
            l^=i;
        }
        for(int i=1;i<encoded.size();i++){
            int a=(encoded[i]^v[i-1]);
            v.push_back(a);
            s^=a;
        }
        f=l^s;
        vector<int>v1;
        v1.push_back(f);
        for(int i=0;i<v.size();i++)
        {
            v1.push_back(v[i]^f);
        }
        return v1;
    }
};
