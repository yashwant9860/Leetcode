class Solution {
    int backtrack(vector<int>&visited,int n,int ind)
    {
        if(ind == n+1)
            return 1;
        int ans=0;
        for(int i=0;i<n;i++)
        {
            if(visited[i]==0&&((i+1)%ind==0||ind%(i+1)==0))
            {
                visited[i]=1;
                ans+=backtrack(visited,n,ind+1);
                visited[i]=0;
            }
        }
        return ans;
    }
public:
    int countArrangement(int n) {
        vector<int>visited(n,0);
        return backtrack(visited,n,1);
    }
};
