class Solution {
   
public:
    bool isScramble(string s1, string s2) {
        int n=s1.length();
        int dp[n][n][n+1];
        memset(dp,0,sizeof dp);
        for(int k=1;k<n+1;k++)
        {
            for(int i=0;i<=n-k;i++)
            {
                for(int j=0;j<=n-k;j++)
                {
                   if(k==1)
                   {
                       dp[i][j][k]=(s1[i]==s2[j]);
                   }
                    else
                    {
                        for(int q=1;q<k&&!dp[i][j][k];q++)
                        {
                            dp[i][j][k]=((dp[i][j][q]&&dp[i+q][j+q][k-q])||(dp[i][j+k-q][q]&&dp[i+q][j][k-q]));
                        }
                    }
                }
            }
        }
        return dp[0][0][n];
    }
};
