class Solution {
public:
    vector<int> findOriginalArray(vector<int>& changed) {
        int n=changed.size();
        sort(changed.begin(),changed.end());
        vector<int>v;
        if(n%2==1)
            return v;
        map<int,int>m;
        for(int i=0;i<n;i++)
        {
             if(changed[i]%2==0 && m.find(changed[i]/2)!=m.end() && m[changed[i]/2]!=0)
            {
                v.push_back(changed[i]/2);
                m[changed[i]/2]--;
            }
            else
            {
                if(m.find(changed[i])!=m.end())
                    m[changed[i]]++;
                else
                    m.insert({changed[i],1});
            }
        }
        vector<int>v1;
        if(v.size()==n/2)
            return v;
        else
            return v1;
    }
};
