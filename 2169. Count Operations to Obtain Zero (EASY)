class Solution {
public:
    int countOperations(int num1, int num2) {
        int ans=0;
        while(num1!=0&&num2!=0)
        {
            if(num2>num1)
                swap(num2,num1);
            if(num2==1)
            {
                ans+=num1;
                return ans;
            }
            else
            {
                num1=num1-num2;
                ans++;
            }
        }
        return ans;
    }
};
