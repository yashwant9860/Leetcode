class Solution {
public:
    vector<vector<int>> largeGroupPositions(string s) {
        vector<vector<int>>v;
        int n = s.length();
        for(int i=0;i<n;i++)
        {
            int j=i+1;
            while(j<n&&s[i]==s[j])
            {
                j++;
            }
            if(j-i>=3)
                v.push_back(vector<int>{i,j-1});
            i=j-1;
        }
        return v;
    }
};
