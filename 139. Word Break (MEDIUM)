class Solution {
public:
    bool wordBreak(string s, vector<string>& wordDict) {
        int dp[s.length()+1];
        memset(dp,0,sizeof dp);
        dp[0]=1;
        for(int i=0;i<=s.length();i++)
        {
            if(dp[i]==1)
            {
            for(auto a:wordDict)
            {
                string k=s.substr(i,a.length());
                if(k==a)
                    dp[a.length()+i]=1;
            }
                
            }
        }
        return dp[s.length()];
    }
};
