class Solution {
public:
    vector<int> circularPermutation(int n, int start) {
        vector<int>v,ans;
        int c=-1;
        for(int i=0;i<(1<<n);i++)
        {
            v.push_back(i^(i/2));
            if((i^(i/2))==(start))
                c=i;
        }
        for(int i=c;i<(1<<n);i++)
            ans.push_back(v[i]);
        for(int i=0;i<c;i++)
            ans.push_back(v[i]);
       
        return ans;
    }
};
