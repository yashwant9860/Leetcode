class Solution {
public:
    int rotatedDigits(int n) {
        
        int s=0;
        for(int i=1;i<=n;i++)
        {
            int k=i,flag1=0,flag2=0,flag3=0;
            while(k!=0)
            {
                if(k%10==2||k%10==5||k%10==6||k%10==9)
                    flag1=1;
                if(k%10==0||k%10==1||k%10==8)
                    flag2=1;
                if(k%10==3||k%10==4||k%10==7)
                    flag3=1;                
                k/=10;
            }
            if(flag3!=1 && flag1==1)
                s++;
                
        }
        return s;
    }
};
