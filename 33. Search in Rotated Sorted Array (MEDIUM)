class Solution {
public:
    int search(vector<int>& nums, int target) {
        int n=nums.size();
        int l=0,r=n-1,mid;
        while(l<r)
        {
            mid=l+(r-l)/2;
            if(nums[mid]>nums[r])
                l=mid+1;
            else
                r=mid;
        }
        int rot=r;
        l=0,r=n-1;
        while(l<=r)
        {
            mid=l+(r-l)/2;
            if(nums[(mid+rot)%n]==target)
                return (mid+rot)%n;
            if(nums[(mid+rot)%n]>target)
            {
                r=mid-1;
            }
            else
                l=mid+1;
        }
        
        return -1;
    }
};
