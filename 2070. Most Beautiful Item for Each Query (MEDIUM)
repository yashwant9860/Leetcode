class Solution {
public:
    vector<int> maximumBeauty(vector<vector<int>>& items, vector<int>& queries) {
        sort(items.begin(),items.end());
        int n=items.size(),o=0;
        vector<int>v;
        for(int i=0;i<n;i++)
        {
            if(items[i][1]<o)
                items[i][1]=o;
            else
                o=items[i][1];
        }
        for(auto a:queries)
        {
            int l=0,r=n-1,mid;
            if(items[0][0]>a)
                v.push_back(0);
            else if(items[n-1][0]<a)
            {
                v.push_back(items[n-1][1]);
            }
                else{
            while(l<=r)
            {
                mid=l+(r-l)/2;
                if(items[mid][0]<=a&&(mid+1==n||items[mid+1][0]>a))
                {
                    v.push_back(items[mid][1]);
                    break;
                }
                else if(items[mid][0]>a)               
                    r=mid-1;
                else
                    l=mid+1;
            }
            }
        }
        return v;
    }
};
