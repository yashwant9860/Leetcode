class MyCalendar {
    set<pair<int,int>>s;
public:
    MyCalendar() {
        
    }
    
    bool book(int start, int end) {
       auto a=s.upper_bound({start,start});
        if(a!=s.end()&&(*a).second<end)return false;
        s.insert(make_pair(end,start));
        return true;   
    }
};

/**
 * Your MyCalendar object will be instantiated and called as such:
 * MyCalendar* obj = new MyCalendar();
 * bool param_1 = obj->book(start,end);
 */
