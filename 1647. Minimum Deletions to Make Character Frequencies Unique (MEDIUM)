class Solution {
public:
    int minDeletions(string s) {
        int hash[26];
        memset(hash,0,sizeof hash);
        for(int i=0;i<s.length();i++)
        {
            int c=s[i]-'a';
            hash[c]++;
        }
        map<int,int>m;
        for(int i=0;i<26;i++)
        {
            if(m.find(hash[i])!=m.end())
                m[hash[i]]++;
            else
                m.insert({hash[i],1});
                
        }
        int sum=0;
        for(int i=0;i<26;i++)
        {
            if(m[hash[i]]>1 && hash[i]!=0)
            {
                int k=hash[i]-1,j=0;
                while(1)
                {
                    j++;
                    if(m.find(k)==m.end()||k==0)
                    {
                        m.insert({k,1});
                        m[hash[i]]--;
                        break;
                    }
                    k--;
                }
                cout<<j<<endl;
                sum+=j;
            }
        }
        return sum;
        
    }
};
