class Solution {
public:
    bool searchMatrix(vector<vector<int>>& matrix, int target) {
        
        int m=matrix.size();
        int n=matrix[0].size();
        for(int i=0;i<m;i++)
        {
            if((target>=matrix[i][0]) && (target<=matrix[i][n-1]));
            {
                cout<<matrix[i][0]<<" "<<matrix[i][n-1]<<" "<<target<<endl;
                cout<<i<<endl;
                int l=0,r=n-1,mid;
                while(l<=r)
                {
                    
                    mid=l+(r-l)/2;
                    cout<<matrix[i][mid]<<endl;
                    if(matrix[i][mid]==target)
                        return true;
                    if(matrix[i][mid]>target)
                    {
                        r=mid-1;
                    }
                    else
                    {
                        l=mid+1;
                    }
                }
            }
        }
        return false;
    }
};
