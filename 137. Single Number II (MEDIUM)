class Solution {
public:
    int singleNumber(vector<int>& nums) {
        vector<int>v(32,0);
        for(auto a:nums)
        {
            bitset<32>b(a);
            for(int i=0;i<32;i++)
            {v[i]+=b[31-i];}
        }
        long long ans=0,k=(long long)1<<30;
        if(v[0]%3!=0)
            ans+=((long long)1<<31)*(-1);
        
        for(int i=1;i<32;i++)
        {
            if(v[i]%3!=0)
                ans+=k;
            k=k>>1;
        }
        return ans; 
    }
};
