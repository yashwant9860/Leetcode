class Solution {
public:
    int digArtifacts(int n, vector<vector<int>>& artifacts, vector<vector<int>>& dig) {
        vector<vector<int>>v(n,vector<int>(n,0));
        
        for(auto a:dig)
        {
            v[a[0]][a[1]]=1;
        }
        
        int ans=0;
        for(auto a:artifacts)
        {
            int flag=0;
            for(int i=a[0];i<=a[2];i++)
            {
                for(int j=a[1];j<=a[3];j++)
                {
                    if(v[i][j]==0)
                    {flag=1;break;}
                }
                if(flag==1)
                    break;
            }
            if(flag==0)
                ans++;
        }
        return ans;
    }
};
