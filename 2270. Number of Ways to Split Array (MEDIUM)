class Solution {
public:
    int waysToSplitArray(vector<int>& nums) {
        long long s=0,sum=0;
        int ans=0,n=nums.size();
        for(auto a:nums)s+=a;
        for(int i=0;i<n-1;i++)
        {
            sum+=nums[i];
            if(s-sum<=sum)
                ans++;
        }
        return ans;
    }
};
