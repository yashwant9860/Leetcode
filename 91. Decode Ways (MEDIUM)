class Solution {

public:
    int numDecodings(string s) {
        int n=s.length();
        vector<int>v(n+1,0);
        v[0]=1;
        if(s[0]!='0')
        v[1]=1;
        for(int i=2;i<n+1;i++)
        {
            if(s[i-1]!='0')
                v[i]+=v[i-1];
            
            int f=s[i-2]-'0',f2=s[i-1]-'0';
            if(f*10+f2<=26 && f*10+f2>9)
                v[i]+=v[i-2];
            
        }
        return v[n];
    }
};
