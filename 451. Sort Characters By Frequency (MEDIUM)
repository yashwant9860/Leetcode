class Solution {
public:
    string frequencySort(string s) {
        vector<pair<int,char>>h(62,{0,'z'});
        for(int i=0,n=s.length();i<n;i++)
        {
            if(s[i]<='z'&&s[i]>='a'){
            h[s[i]-'a'].first++;
            h[s[i]-'a'].second=s[i];}
            else if(s[i]>='A'&&s[i]<='Z')
            {
                h[s[i]-'A'+26].first++;
                h[s[i]-'A'+26].second=s[i];
            }
            else
            {
                h[s[i]-'0'+52].first++;
                h[s[i]-'0'+52].second=s[i];
            }
        }
        sort(h.begin(),h.end(),greater<pair<int,char>>{});
        string p="";
        for(auto a:h)
        {
            if(a.first==0)
                return p;
            int k=a.first;
            while(k--)
                p+=a.second;
        }
        return p;
    }
};
