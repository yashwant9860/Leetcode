class Solution {
public:
    int nthUglyNumber(int n) {
        vector<int>v;
        int a=0,b=0,c=0;
        for(int i=0;i<n;i++)
        {
            if(i==0)
            {
                v.push_back(1);
                continue;
            }
            int m=min(min(v[a]*2,v[b]*3),v[c]*5);
            v.push_back(m);
            if(m==v[a]*2)a++;
            if(m==v[b]*3)b++;
            if(m==v[c]*5)c++;
        }
        return v[n-1];
    }
};
