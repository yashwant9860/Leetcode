class Solution {
public:
    int firstMissingPositive(vector<int>& nums) {
        int n=nums.size();
        vector<int>v(n+1,0);
        for(auto a:nums)
            if(a>0&&a<=n)
                v[a]++;
        for(int i=1;i<n+1;i++)
            if(v[i]==0)
                return i;
        return n+1;
            
    }
};
