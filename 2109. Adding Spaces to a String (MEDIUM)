class Solution {
public:
    string addSpaces(string s, vector<int>& spaces) {       
        int d=spaces.size(),n=s.length();
        string ans="";
        for(int i=0,j=0;i<s.length();i++)
        {
            if(j<d &&spaces[j]==i)
            {ans+=' ';j++;i--;}
            else
            ans+=s[i];
        }
        return ans;
        
    }
};
