class Solution {
public:
    int uniquePaths(int m, int n) {
        long long arr[m][n];
       memset(arr,0,sizeof arr);
            for(int j=0;j<n;j++)
                arr[0][j]=1;
          for(int j=0;j<m;j++)
                arr[j][0]=1;
        for(int i=1;i<m;i++)
        {
            for(int j=1;j<n;j++)
            {
                arr[i][j]+=arr[i-1][j];                
                arr[i][j]+=arr[i][j-1];   
             }
        }
        return arr[m-1][n-1];
    }
};
