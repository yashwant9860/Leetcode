class Solution {
public:
    bool isSubsequence(string s, string t) {
        int l1=s.length(),l2=t.length();
        int j=0;
        for(int i=0;i<l2;i++)
        {
            if(s[j]==t[i])
                j++;
            if(j==l1)
                return true;
        }
        return s==t;
        
    }
};
