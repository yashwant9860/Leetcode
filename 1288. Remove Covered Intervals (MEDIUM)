class Solution {
    
  static  bool sortbysec(const vector<int> &a,const vector<int> &b)
{
      if(a[0]!=b[0])
          return a[0]<b[0];
      else
    return  a[1]>b[1];
}
public:
    int removeCoveredIntervals(vector<vector<int>>& intervals) {
        sort(intervals.begin(), intervals.end(), sortbysec);
        int n=intervals.size(),ans=0;
        for(int i=0;i<n;)
        {
            int j=i;
            i=i+1;
            while( i<n&& intervals[j][0]<=intervals[i][0] && intervals[j][1]>=intervals[i][1])
            {
                i++;
            }
            ans++;
            cout<<intervals[j][0]<<" "<<intervals[j][1]<<endl;
        }
        return ans;
    }
};
