class Solution {
public:
    int findKthLargest(vector<int>& nums, int k) {
        priority_queue<int,vector<int>,greater<int>>pq;
        for(auto a:nums)
        {
            if(k>0)
            {
                pq.push(a);
                k--;
            }
            else
            {
                if(pq.top()<a)
                {
                    pq.pop();
                    pq.push(a);
                }
            }
        }
        return pq.top();
    }
};
