class Solution {
    void f(vector<int>&nums,int currind,vector<int>a,set<vector<int>>&s)
    {
        int n=nums.size();
        if(a.size()>1)
        s.insert(a);
        if(currind==n)
            return ;
        for(int i=currind;i<n;i++)
        {
            if(a.size()==0)
                a.push_back(nums[i]);
            else
            {
                if(a[a.size()-1]<=nums[i])
                    a.push_back(nums[i]);
            }
            f(nums,i+1,a,s);
            a.pop_back();
        }
    }
public:
    vector<vector<int>> findSubsequences(vector<int>& nums) {
        set<vector<int>>s;
        vector<vector<int>>ans;
        vector<int>a;
        f(nums,0,a,s);
        for(auto &a:s)
        {
            ans.push_back(a);
        }
        return ans;
    }
};
