class RandomizedSet {
    unordered_map<int,int>m;
    vector<int>v;
    int n=1;
public:
    RandomizedSet() {
        
    }
    
    bool insert(int val) {
        if(m[val]!=0)
            return false;
        m[val]=n;
        n++;
        v.push_back(val);
        return true;
    }
    
    bool remove(int val) {
        if(m[val]==0)
            return false;
        int a=m[val];
        n--;
        v[a-1]=v[n-1];
        m[v[n-1]]=a;
        m[val]=0;
        v.pop_back();
        return true;
    }
    
    int getRandom() {
        int r=rand()%(n-1);
        return v[r];
    }
};

/**
 * Your RandomizedSet object will be instantiated and called as such:
 * RandomizedSet* obj = new RandomizedSet();
 * bool param_1 = obj->insert(val);
 * bool param_2 = obj->remove(val);
 * int param_3 = obj->getRandom();
 */
