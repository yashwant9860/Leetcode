class Solution {
public:
    vector<bool> camelMatch(vector<string>& queries, string pattern) {
        int n=queries.size(),p=pattern.length();
        vector<bool>v(n,0);
        for(int i=0;i<n;i++)
        {
            int count=0,ql=queries[i].length(),t=0;
            for(int j=0;j<ql;j++)
            {
                if(t<p&&queries[i][j]==pattern[t] )
                    t++;
                else if(queries[i][j]>='A'&&queries[i][j]<='Z')
                {break;}
                else
                    count++;
            }
            if(count==ql-t&&t==p)
                v[i]=1;
            else
                v[i]=0;
        }
        return v;
    }
};
