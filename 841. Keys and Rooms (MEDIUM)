class Solution {
public:
    bool canVisitAllRooms(vector<vector<int>>& rooms) {
        int n=rooms.size(),count=1;
        vector<int>visited(n,0);
        visited[0]=1;
        stack<int>s;
        for(auto a:rooms[0])
        { s.push(a);visited[a]=1;count++;}
        while(!s.empty() && count<n)
        {
            int a=s.top();
            s.pop();
            for(auto b:rooms[a])
            {
                if(visited[b]!=1)
                {visited[b]=1;count++;s.push(b);}
            }
        }
        if(n==count)
            return true;
        return false;
    }
};
