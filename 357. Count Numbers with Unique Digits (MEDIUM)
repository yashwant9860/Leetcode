class Solution {
public:
    int countNumbersWithUniqueDigits(int n) {
        vector<int>dp(n+5,1);
        dp[0]=1;
        dp[1]=10;
        dp[2]=91;
        int s=91;
        for(int i=3;i<n+1;i++)
        {
            int j=i-1,k=9;
            dp[i]*=9;
            while(j-->=1)
            {
                dp[i]*=k;
                k--;
            }
            dp[i]+=s;
            s=dp[i];
        }
        return dp[n];
    }
};
