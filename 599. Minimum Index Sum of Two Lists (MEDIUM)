class Solution {
public:
    vector<string> findRestaurant(vector<string>& list1, vector<string>& list2) {
        int ans=INT_MAX;
        map<string,int>m;
        vector<string>v;
        for(int i=0;i<list1.size();i++)m.insert({list1[i],i});
        for(int i=0;i<list2.size();i++)
        {
            if(m.find(list2[i])!=m.end())
            {ans=min(ans,i+m[list2[i]]);}
        }
        for(int i=0;i<list2.size();i++)
        {
            if(m.find(list2[i])!=m.end())
            {
                if(ans==m[list2[i]]+i)
                    v.push_back(list2[i]);
            }
        }
        return v;
        
    }
};
